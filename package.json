{
  "name": "node-es6-restapi-boilerplate",
  "version": "0.0.1",
  "description": "Express boilerplate for building RESTful APIs",
  "author": "keviveks <keviveks@gmail.com>",
  "main": "src/index.js",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": "8.11.4",
    "yarn": "*"
  },
  "scripts": {
    "start": "nodemon ./index.js",
    "lint": "eslint **/*.js --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint -- --fix",
    "lint:watch": "yarn lint -- --watch",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 20000 --recursive src/modules/*/tests/*/",
    "test:unit": "NODE_ENV=test mocha src/modules/user/tests/unit",
    "test:integration": "NODE_ENV=test mocha --timeout 20000 src/modules/*/tests/integration",
    "test:watch": "NODE_ENV=test mocha --watch src/api/tests/unit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "script": "node src/commander.js",
    "docs": "apidoc -i src -o docs",
    "postdocs": "opn docs/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:keviveks/node-starter-boilerplate.git"
  },
  "keywords": [
    "node",
    "node.js",
    "express",
    "mongodb",
    "mongoose",
    "passport",
    "es6",
    "es7",
    "es8",
    "es2017",
    "eslint",
    "REST",
    "API",
    "boilerplate",
    "generator",
    "starter project"
  ],
  "_moduleAliases": {
    "@root": ".",
    "@configs": "src/configs",
    "@system": "src/system",
    "@modules": "src/modules"
  },
  "dependencies": {
    "bcrypt": "^3.0.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "commander": "^2.16.0",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-validation": "^1.0.2",
    "glob": "^7.1.2",
    "helmet": "^3.12.1",
    "http-status": "^1.2.0",
    "inquirer": "^6.0.0",
    "joi": "^13.4.0",
    "joi-objectid": "^2.0.0",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.10",
    "method-override": "^2.3.10",
    "module-alias": "^2.1.0",
    "moment": "^2.22.2",
    "mongoose": "^5.1.4",
    "mongoose-delete": "^0.4.0",
    "mongoose-id-validator": "^0.4.3",
    "morgan": "^1.9.0",
    "morgan-body": "^2.4.5",
    "nodemailer": "^4.6.5",
    "onesignal-node": "^1.1.2",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth2": "^0.1.6",
    "passport-jwt": "^4.0.0",
    "passport-linkedin": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pug": "^2.0.3",
    "query-types": "^0.1.4",
    "random-number-csprng": "^1.0.2",
    "textlocal": "^0.1.1",
    "winston": "^2.4.2",
    "winston-daily-rotate-file": "^3.2.1"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "data-driven": "^1.4.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "faker": "^4.1.0",
    "mocha": "^5.2.0",
    "mongoose-seeder": "^1.2.1",
    "nodemon": "^1.17.5",
    "nyc": "^12.0.2",
    "opn-cli": "^3.1.0",
    "rewire": "^4.0.1",
    "sinon": "^6.1.4",
    "sinon-chai": "^3.2.0",
    "sinon-mongoose": "^2.2.1",
    "supertest": "^3.1.0"
  }
}
